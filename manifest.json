{
    "manifest_version": 3,
    "name": "Obsidian Clipper",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["*://*/*"]
        }
    ],
    "permissions": ["activeTab", "scripting"]
}
